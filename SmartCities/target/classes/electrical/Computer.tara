dsl Tafat
use Residential

Behavior ElectricalConsumer
	on Computer
		var measure:Power activePower = 0 W
		var measure:Power standByPower = 7.1 W
		var measure:Power installedPower = 100 W
		var word:[Off StandBy On] mode = Off
		Action('mode() == Mode.Off', 'power(0)')
		Action('mode() == Mode.StandBy', 'power(StandBy)')
		Action('mode() == Mode.On', 'power(installedPower)')
