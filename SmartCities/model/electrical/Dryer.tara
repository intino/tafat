dsl Tafat
use Residential


Behavior ElectricalConsumer
	on Dryer
		var measure:Power activePower = 0 W
		var measure:Power installedPower = 2500 W
		var word:[Off On] mode = Off
		StateChart
			State Off > EntryAction('activePower(0)')
			State On > EntryAction('activePower(installedPower())')
			Transition(Off, On) > Condition('mode() == Mode.Off')
			Transition(On, Off) > After(72 m)