dsl Tafat

use Main

Behavior Consumer on Family
	sub SimpleConsumer
		TableFunction(data = "") > LinearInterpolation; NoneExtrapolation
			Data(0, 0)
			Data(6, 0)
			Data(8, 200)
			Data(14, 400)
			Data(20, 300)
			Data(23, 0)
	sub ComplexConsumer
		TableFunction(data = "") > LinearInterpolation; LinearExtrapolation
			Data... with Consumption(hour, power)
