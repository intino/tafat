		Square(value = 'return self.graph().rootList(Fridge.class).get(4).power();', top = 19, left = 1247, width = 250 , height = 450, zIndex = -1)
	Heatmap("Heatmap With Circle", background = "ui/fridgesmap.png", color = Blue)
