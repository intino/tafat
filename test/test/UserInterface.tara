		Circle(value = 'return self.graph().rootList(Fridge.class).get(3).power();', centerX= 1053, centerY = 270, diameter = 250, zIndex = -1)
		Circle(value = 'return self.graph().rootList(Fridge.class).get(4).power();', centerX= 1375, centerY = 270, diameter = 250, zIndex = -1)

